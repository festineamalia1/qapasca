	.wrap-mainnav .mainnav:hover{background-color: #C1392D;color: white;
		-webkit-transition: background-color 0.5s ease-in 0s, color 0.3s ease-in 0s;
		-moz-transition: background-color 0.5s ease-in 0s, color 0.3s ease-in 0s;
		-o-transition: background-color 0.5s ease-in 0s, color 0.3s ease-in 0s;
		transition: background-color 0.5s ease-in 0s, color 0.3s ease-in 0s;}	
	.wrap-mainnav .separator{float:left; background: transparent url(../images/pemisah_bg_mainnav.jpg) no-repeat 0 0px; 
		width: 2px; height: 42px;}
		
		
	.wrap-mainnav ul.wrap-submainnav{position: absolute; left: -5px; top: 35px; z-index: 99;
		width: 200px; padding: 5px 0px;		
		background: none repeat scroll 0 0 padding-box rgba(117, 10, 15, 0.85);
		border: 1px solid rgba(100, 100, 100, 0.4);border-radius: 5px;
		box-shadow: 0 3px 8px rgba(0, 0, 0, 0.25);display: none;}


		ul.wrap-submainnav li.submainnav{position:relative; float:left; width: 100%;}
		ul.wrap-submainnav li.submainnav a.submenulabel{float:left; width: 80%; padding:10px 10%; color: white;
			border-bottom: 1px solid #5f0206;}
		ul.wrap-submainnav li.submainnav:hover{background: none repeat scroll 0 0 padding-box rgba(117, 10, 15, 1);}


	.wrap-mainnav ul.wrap-submainnav2{position: absolute; right: -200px; top: -5px; z-index: 99;
		width: 200px; padding: 5px 0px;		
		background: none repeat scroll 0 0 padding-box rgba(117, 10, 15, 0.85);
		border: 1px solid rgba(100, 100, 100, 0.4);border-radius: 5px;
		box-shadow: 0 3px 8px rgba(0, 0, 0, 0.5);display: none;}
	.wrap-mainnav ul.wrap-submainnav2 li.submainnav2 a.submenulabel2{float:left; width: 80%; padding:10px 10%; color: white;
			border-bottom: 1px solid #5f0206;}	
	ul.wrap-submainnav2 li.submainnav2 a.submenulabel2:hover{background: none repeat scroll 0 0 padding-box rgba(117, 10, 15, 1);}
	
	ul.wrap-submainnav li.submainnav a.last,ul.wrap-submainnav2 li.submainnav2 a.last{border-bottom: none !important; }
	.wrap-mainnav .mainnav:hover ul.wrap-submainnav{display: block;}		
	ul.wrap-submainnav li.submainnav:hover ul.wrap-submainnav2{display: block;}


