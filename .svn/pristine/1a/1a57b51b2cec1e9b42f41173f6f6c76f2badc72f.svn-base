	width: 1000px;  margin: 20px 0px 0px 0px; font-size: 12px;height:42px; border-radius:10px 10px 0px 0px;-moz-border-radius: 10px 10px 0px 0px;}

	ul.wrap-submainnav .arrownav{position: absolute; z-index: 4; top: -8px; left: 40px;
		background: url(../images/arrow_nav.png) no-repeat 0 0; width: 16px; height: 8px;}
	ul.wrap-submainnav .arrownav2{position: absolute; z-index: 4; top: 12px; left: -6px;
		background: url(../images/arrow_nav2.png) no-repeat 0 0; width: 8px; height: 16px;}
	
	.wrap-mainnav .mainnav{float: left; position: relative;height:42px; }
	.wrap-mainnav .home{ border-radius:10px 0px 0px 0px; border-radius:10px 0px 0px 0px;-moz-border-radius: 10px 0px 0px 0px;}
