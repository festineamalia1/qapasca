  `sumber_data` varchar(50) DEFAULT NULL,
  `id_sumber_data` int(5) DEFAULT NULL,
  `lampiran` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`id_penjelasan`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=11 ;

--
-- Dumping data for table `penjelasan_pengelolaan_dana`
