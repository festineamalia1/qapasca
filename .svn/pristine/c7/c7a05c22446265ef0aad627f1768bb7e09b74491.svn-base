CREATE TABLE IF NOT EXISTS `suasana_akademik` (
  `id_suasana` int(5) NOT NULL AUTO_INCREMENT,
  `id_prodi` int(5) NOT NULL,
  `id_administrasi` int(5) NOT NULL,
  `kebijakan` longtext NOT NULL,
  `sarana_dana` longtext NOT NULL,
  `kegiatan` longtext NOT NULL,
  `interaksi` longtext NOT NULL,
  `pengembangan_perilaku` longtext NOT NULL,
  `ket_lainnya` longtext NOT NULL,
  `F_pembelajaran` longtext NOT NULL,
  `F_suasana_akademik` longtext NOT NULL,
  `sumber_data` varchar(100) NOT NULL,
  `id_sumber_data` int(5) DEFAULT NULL,
  `lampiran` varchar(100) NOT NULL,
  PRIMARY KEY (`id_suasana`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=6 ;

