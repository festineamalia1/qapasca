  `semester` varchar(5) NOT NULL,
  `nama_mk` varchar(100) NOT NULL,
  `bobot_sks` decimal(5,0) NOT NULL,
  `bobot_jenis_sks` varchar(255) DEFAULT NULL,
  `jenis_sks` enum('inti','institusional') NOT NULL,
  `bobot_tgs` enum('ya','tidak') NOT NULL,
  `kelengkapan_deskripsi` enum('ya','tidak') NOT NULL,
  `kelengkapan_silabus` enum('ya','tidak') NOT NULL,
  `kelengkapan_SAP` enum('ya','tidak') NOT NULL,
  `penyelenggara` varchar(30) NOT NULL,
  `ket_tambahan` varchar(30) NOT NULL,
  `sumber_data` varchar(100) NOT NULL,
  `id_sumber_data` int(5) NOT NULL,
  `lampiran` varchar(100) NOT NULL,
  PRIMARY KEY (`kode_mk`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

