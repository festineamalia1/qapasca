CREATE TABLE IF NOT EXISTS `umpan_balik` (
  `id_umpan_balik` int(11) NOT NULL AUTO_INCREMENT,
  `id_prodi` int(5) NOT NULL,
  `id_administrasi` int(5) NOT NULL,
  `umpan_balik_dosen` longtext NOT NULL,
  `tindak_lanjut_dosen` longtext NOT NULL,
  `umpan_balik_mhs` longtext NOT NULL,
  `tindak_lanjut_mhs` longtext NOT NULL,
  `umpan_balik_alumni` longtext NOT NULL,
  `tindak_lanjut_alumni` longtext NOT NULL,
  `umpan_balik_lulusan` longtext NOT NULL,
  `tindak_lanjut_lulusan` longtext NOT NULL,
  `sumber_data` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id_umpan_balik`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=7 ;

