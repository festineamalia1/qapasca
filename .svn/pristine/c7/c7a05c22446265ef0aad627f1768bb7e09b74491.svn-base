--
-- Table structure for table `kerjasama`
--

CREATE TABLE IF NOT EXISTS `kerjasama` (
  `id_kerjasama` int(5) NOT NULL AUTO_INCREMENT,
  `id_prodi` int(5) NOT NULL,
  `id_administrasi` int(5) NOT NULL,
  `nama_instansi` varchar(225) NOT NULL,
  `jenis_kegiatan` text NOT NULL,
  `th_mulai` year(4) NOT NULL,
  `th_akhir` year(4) NOT NULL,
  `manfaat` text NOT NULL,
  `jenis_kerjasama` enum('dlm negeri','luar negeri') NOT NULL,
  `sumber_data` varchar(150) NOT NULL,
  `id_sumber_data` int(5) DEFAULT NULL,
  `lampiran` varchar(150) NOT NULL,
  PRIMARY KEY (`id_kerjasama`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=141 ;

