  `id_kepemimpinan` int(11) NOT NULL AUTO_INCREMENT,
  `id_prodi` int(5) DEFAULT NULL,
  `id_administrasi` int(5) DEFAULT NULL,
  `kepemimpinan` longtext NOT NULL,
  `sumber_data` varchar(100) NOT NULL,
  PRIMARY KEY (`id_kepemimpinan`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=13 ;

