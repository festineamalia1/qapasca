  `status_dosen` enum('tetap','tidak tetap') NOT NULL,
  `status_bidang` enum('sesuai PS','tdk sesuai PS') NOT NULL,
  `sumber_data` varchar(100) NOT NULL,
  `id_sumber_data` int(5) DEFAULT NULL,
  `lampiran` varchar(100) NOT NULL,
