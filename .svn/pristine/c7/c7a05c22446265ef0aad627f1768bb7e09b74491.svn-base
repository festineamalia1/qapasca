CREATE TABLE IF NOT EXISTS `pelaksanaan_bimbingan_ta` (
  `id_pelaksanaan` int(5) NOT NULL AUTO_INCREMENT,
  `id_prodi` int(5) DEFAULT NULL,
  `id_administrasi` int(5) DEFAULT NULL,
  `rata_mhs_perdosen` varchar(50) DEFAULT NULL,
  `rata_jum_pertemuan` varchar(50) DEFAULT NULL,
  `ketersediaan_panduan` enum('tidak','ya') DEFAULT NULL,
  `sosialisasi_pelaksanaan` longtext,
  `sumber_data` varchar(50) DEFAULT NULL,
  `id_sumber_data` int(5) DEFAULT NULL,
  `lampiran` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`id_pelaksanaan`)
) ENGINE=MyISAM  DEFAULT CHARSET=latin1 AUTO_INCREMENT=9 ;

